{
  "overwrite": true,
  "schema": "http://localhost:4000/graphql",
  "documents": "src/**/*.{gql,graphql}",
  "generates": {
    "src/generated/": {
      "preset": "client-preset",
      "plugins": [
      "typescript-react-apollo"
      ],
      "config": {
        "naminConvention": "dotCase",
        "dedupeFragments": true,
        "skipTypename": true,
        "withHooks": true,
        "noExport": true
      }
    }
  }
}
